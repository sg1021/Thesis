    <Routine name="trialCode">
      <CodeComponent name="codeTrial">
        <Param name="Before Experiment" updates="constant" val="import random, xlrd&amp;#10;&amp;#10;#randamize the seed&amp;#10;random.seed()&amp;#10;&amp;#10;#stimulus file&amp;#10;infile = 'Simple_Word.xlsx'&amp;#10;&amp;#10;#total num of  items&amp;#10;num_items= 25&amp;#10;&amp;#10;&amp;#10;" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="import * as random from 'random';&amp;#10;import * as xlrd from 'xlrd';&amp;#10;var infile, num_items;&amp;#10;random.seed();&amp;#10;infile = &quot;Simple_Word.xlsx&quot;;&amp;#10;num_items = 25;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="&amp;#10;#access the xls the stimulus file&amp;#10;inbook = xlrd.open_workbook(infile)&amp;#10;#Need to define &quot;Sheet&quot; in excel (machine counts 0  as 1)&amp;#10;insheet = inbook.sheet_by_index(0)&amp;#10;&amp;#10;#arrays for our stimuli&amp;#10;word = []&amp;#10;&amp;#10;#read the stimuli from our sheet&amp;#10;for rowx in range(num_items):&amp;#10;    &amp;#10;    #read in the values of all columns on this row&amp;#10;    row = insheet.row_values(rowx)&amp;#10;    &amp;#10;    #saving the word to the word array&amp;#10;    word.append(row)&amp;#10;    &amp;#10;#randomizes the order of the word array&amp;#10;random.shuffle(word)&amp;#10;&amp;#10;#make each array&amp;#10;WordItem = []&amp;#10;WordIndex = []&amp;#10;WordGroup = []&amp;#10;&amp;#10;#assigning the word item, index, and group&amp;#10;for i in range(num_items):&amp;#10;    WordItem.append(word[i][0])&amp;#10;    WordIndex.append(word[i][1])&amp;#10;    WordGroup.append(word[i][2])&amp;#10;    &amp;#10;current_trial = 0" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="inbook = xlrd.open_workbook(infile);&amp;#10;insheet = inbook.sheet_by_index(0);&amp;#10;word = [];&amp;#10;for (var rowx = 0, _pj_a = num_items; (rowx &lt; _pj_a); rowx += 1) {&amp;#10;    row = insheet.row_values(rowx);&amp;#10;    word.append(row);&amp;#10;}&amp;#10;random.shuffle(word);&amp;#10;WordItem = [];&amp;#10;WordIndex = [];&amp;#10;WordGroup = [];&amp;#10;for (var i = 0, _pj_a = num_items; (i &lt; _pj_a); i += 1) {&amp;#10;    WordItem.append(word[i][0]);&amp;#10;    WordIndex.append(word[i][1]);&amp;#10;    WordGroup.append(word[i][2]);&amp;#10;}&amp;#10;current_trial = 0;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="Variable_Item = WordItem[current_trial];&amp;#10;Variable_Index = WordIndex[current_trial];&amp;#10;Variable_Group = WordGroup[current_trial];&amp;#10;current_trial = (current_trial + 1);&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="#make a variable for each to be called from components&amp;#10;Variable_Item = WordItem[current_trial]&amp;#10;Variable_Index = WordIndex[current_trial]&amp;#10;Variable_Group = WordGroup[current_trial]&amp;#10;&amp;#10;current_trial = current_trial + 1 " valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
