        <Param name="Begin Experiment" updates="constant" val="#1. Import the stimulus file&amp;#10;#2. Set up for LSL&amp;#10;&amp;#10;import random, xlrd&amp;#10;from pylsl import StreamInfo, StreamOutlet&amp;#10;&amp;#10;#randamize the seed&amp;#10;random.seed()&amp;#10;#stimulus file&amp;#10;infile = 'Simple_Word.xlsx'&amp;#10;#total num of  items&amp;#10;num_items= 25&amp;#10;&amp;#10;&amp;#10;#Setup for LSL &amp;#10;info = StreamInfo(name='WordStimulus', type='Markers', channel_count=1,&amp;#10;                  channel_format='int32', source_id='uniqueid12345')&amp;#10;# Initialize the stream.&amp;#10;outlet = StreamOutlet(info)&amp;#10;&amp;#10;&amp;#10;# ###########&amp;#10;#1. make the access to the xlsx file&amp;#10;#2. make a file by appending each values from xlsx&amp;#10;&amp;#10;#3. make each array including each field (item, index, group)&amp;#10;&amp;#10;#access the xls the stimulus file&amp;#10;inbook = xlrd.open_workbook(infile)&amp;#10;#Need to define &quot;Sheet&quot; in excel (machine counts 0  as 1)&amp;#10;insheet = inbook.sheet_by_index(0)&amp;#10;&amp;#10;#arrays for our stimuli&amp;#10;word = []&amp;#10;&amp;#10;#initialize the counter &amp;#10;#rowx = 0&amp;#10;#read the stimuli from our sheet&amp;#10;for rowx in range(num_items):&amp;#10;    &amp;#10;    #read in the values of all columns on this row&amp;#10;    row = insheet.row_values(rowx)&amp;#10;    &amp;#10;    #saving the word to the word array&amp;#10;    word.append(row)&amp;#10;&amp;#10; # USED TO BE HERE&amp;#10;#randomizes the order of the word array&amp;#10;random.shuffle(word)&amp;#10;&amp;#10;#make each array&amp;#10;WordItem = []&amp;#10;WordIndex = []&amp;#10;WordGroup = []&amp;#10;&amp;#10;#assigning the word item, index, and group&amp;#10;for i in range(num_items):&amp;#10;    WordItem.append(word[i][0])&amp;#10;    WordIndex.append(word[i][1])&amp;#10;    WordGroup.append(word[i][2])&amp;#10;&amp;#10;current_trial = 0" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="import * as random from 'random';&amp;#10;import * as xlrd from 'xlrd';&amp;#10;import {StreamInfo, StreamOutlet} from 'pylsl';&amp;#10;var WordGroup, WordIndex, WordItem, current_trial, inbook, infile, info, insheet, num_items, outlet, row, word;&amp;#10;random.seed();&amp;#10;infile = &quot;Simple_Word.xlsx&quot;;&amp;#10;num_items = 25;&amp;#10;info = new StreamInfo({&quot;name&quot;: &quot;WordStimulus&quot;, &quot;type&quot;: &quot;Markers&quot;, &quot;channel_count&quot;: 1, &quot;channel_format&quot;: &quot;int32&quot;, &quot;source_id&quot;: &quot;uniqueid12345&quot;});&amp;#10;outlet = new StreamOutlet(info);&amp;#10;inbook = xlrd.open_workbook(infile);&amp;#10;insheet = inbook.sheet_by_index(0);&amp;#10;word = [];&amp;#10;for (var rowx = 0, _pj_a = num_items; (rowx &lt; _pj_a); rowx += 1) {&amp;#10;    row = insheet.row_values(rowx);&amp;#10;    word.append(row);&amp;#10;}&amp;#10;random.shuffle(word);&amp;#10;WordItem = [];&amp;#10;WordIndex = [];&amp;#10;WordGroup = [];&amp;#10;for (var i = 0, _pj_a = num_items; (i &lt; _pj_a); i += 1) {&amp;#10;    WordItem.append(word[i][0]);&amp;#10;    WordIndex.append(word[i][1]);&amp;#10;    WordGroup.append(word[i][2]);&amp;#10;}&amp;#10;current_trial = 0;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
